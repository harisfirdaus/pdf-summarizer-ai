{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/pdf-summarizer-gemini/$1"
    }
  ],
  "outputDirectory": "pdf-summarizer-gemini/dist",
  "buildCommand": "cd pdf-summarizer-gemini && npm install && npm run build",
  "installCommand": "cd pdf-summarizer-gemini && npm install"
}